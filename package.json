{
  "name": "@ts-templates/node20",
  "version": "0.1.0",
  "description": "Template for TypeScript project using Node.js v20",
  "private": true,
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint '**/*.{js,ts}'",
    "prebuild": "npm run clean",
    "start": "ts-node src/main.ts",
    "test": "jest --passWithNoTests"
  },
  "engines": {
    "node": "20.10.0"
  },
  "devDependencies": {
    "@eslint-recommended/eslint-config-typescript": "38.0.0",
    "@tsconfig/node20": "20.1.4",
    "@types/node": "20.10.8",
    "eslint": "8.57.0",
    "eslint-plugin-jest": "28.2.0",
    "jest": "29.7.0",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.4"
  },
  "license": "MIT"
}
